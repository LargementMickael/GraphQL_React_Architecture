{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "script"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/no-import-module-exports": ["off"],
        "linebreak-style": "off",
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "no-console": "off",
        "import/prefer-default-export": "off",
        "indent": ["error", 4]
    }
}
